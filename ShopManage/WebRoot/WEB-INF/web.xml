    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CollectStartServlet</servlet-name>
    <servlet-class>Lyn.ShopManage.Servlet.CollectStartServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CollectMemberManageServlet</servlet-name>
    <servlet-class>Lyn.ShopManage.Servlet.CollectMemberManageServlet</servlet-class>
  </servlet>













  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/servlet/IndexServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ConfirmServlet</servlet-name>
    <url-pattern>/confirm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MenuServlet</servlet-name>
    <url-pattern>/menu</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WeChatServlet</servlet-name>
    <url-pattern>/wechat</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WeChatServerServlet</servlet-name>
    <url-pattern>/WeChatServerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CollectServlet</servlet-name>
    <url-pattern>/collectMoney</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CollectStockManageServlet</servlet-name>
    <url-pattern>/CStockManage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CollectFlowingWaterServlet</servlet-name>
    <url-pattern>/CFlowingWater</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CollectStartServlet</servlet-name>
    <url-pattern>/CollectStart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CollectMemberManageServlet</servlet-name>
    <url-pattern>/CMemberManage</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
